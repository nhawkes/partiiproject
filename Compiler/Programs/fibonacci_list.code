
data Cons(head, tail)
data Empty

fibonacciListFrom(a, b){
    fibonacciListFrom(a, fibonacciListFrom(b, a+b))
}
take(i, list){
    switch(list){
        | Cons(head, tail) =>
            {
                switch(i){
                    | 0 => head
                    | _ => take(i-1, tail)
                }
            }
    }

}
export fibonacci(x) = {
    return switch(x){
        | 0 => 1
        | 1 => 1
        | _ => fibonacci(x-1) + fibonacci(x-2)
    }
}